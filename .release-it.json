{
  "git": {
    "requireCleanWorkingDir": true,
    "commitMessage": "chore(release): ${version}",
    "tagName": "${version}",
    "tagAnnotation": "Release ${version}"
  },
  "github": {
    "release": true,
    "releaseName": "Release ${version}",
    "releaseNotes": "Release notes for version ${version}.",
    "tokenRef": "GITHUB_TOKEN"
  },
  "npm": {
    "publish": false
  }
}
